
<footer>
		<div id="bottom-titles">
				 <div id="name">
					 <a href="/" rel="home">
							<h1>{{bloginfo('name')}}</h1>
					 </a>
				</div>
				{% if cpt %}
						<div id="cat_title">
								<h1 class="{{cpt}}">{{cpt_title}}</h1>
